		</async>
		<ee:transform doc:name="response_data" doc:id="1ca5cf94-e2ec-4619-84e7-b81bb6561e65">
			<ee:message>
			</ee:message>
			<ee:variables>
				<ee:set-variable variableName="response_data" ><![CDATA[%dw 2.0
output application/json
---
{
	api_status: "OK"
}]]></ee:set-variable>
			</ee:variables>
		</ee:transform>
	</flow>
</mule>
