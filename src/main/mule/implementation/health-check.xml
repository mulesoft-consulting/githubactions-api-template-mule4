				<ee:message />
				<ee:variables>
					<ee:set-variable variableName="LOG_MESSAGE"><![CDATA[%dw 2.0
output application/json
---
{
	flowName: "get:\health-check:imp",
	correlationId: correlationId,
	category: "REGULAR", //REGULAR SECURITY SYSTEM METRIC
	key: "health-check-status",
	value: "OK"
}
]]></ee:set-variable>
					<ee:set-variable variableName="LOG_LEVEL"><![CDATA[//DEBUG ERROR INFO TRACE WARN
"INFO"]]></ee:set-variable>
				</ee:variables>
